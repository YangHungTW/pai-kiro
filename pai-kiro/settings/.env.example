# PaiKiro Environment Variables
# Copy to .env and fill in your values

# Identity
PAI_USER_NAME=Yang
PAI_ASSISTANT_NAME=PaiLang
DA=PaiLang

# Runtime
PAI_DIR=/Users/your-username/.kiro
TIME_ZONE=Asia/Taipei

# Observability
PAI_OBSERVABILITY_URL=http://localhost:4000/events

# Anytype MCP
# Get from Anytype settings
ANYTYPE_MCP_HEADERS=your_anytype_headers

# GitHub MCP
# Create at https://github.com/settings/tokens
GITHUB_TOKEN=your_github_token
